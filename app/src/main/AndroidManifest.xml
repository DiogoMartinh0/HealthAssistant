<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pt.isec.gps1718.g34.healthassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".View.ViewPrescription">
        </activity>

        <activity android:name=".View.ViewAppointment">
        </activity>

        <activity android:name=".Create.CreatePrescription">
        </activity>

        <activity android:name=".Create.CreateAppointment">
        </activity>
    </application>

</manifest>